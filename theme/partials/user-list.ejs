

      <table class="ui celled table">
        <thead>
        <tr>
          <th>姓名</th>
          <th>性别</th>
          <th>账户类型</th>
          <th>组别</th>
          <th>个性签名</th>
          <th>操作</th>

        </tr></thead>
        <tbody>
        <% users.forEach(function (item) {%>
        <tr>
          <td><%=item.name%></td>
          <td><%=item.gender||""%></td>
          <td><%=item.account_type%></td>
          <td><%=item.group||""%></td>
          <td><%=item.signature||""%></td>
          <td><a href="/user/detail/<%=item._id%>">删除</a></td>

        </tr>
        <%});%>

        </tbody>
      </table>
